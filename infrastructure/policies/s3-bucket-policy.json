{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCloudFrontOAI",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity YOUR-CLOUDFRONT-OAI-ID"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::YOUR-S3-UI-BUCKET/*"
    }
  ]
}
