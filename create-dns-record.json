{
  "Comment": "Create CNAME record for your-custom-domain.com pointing to CloudFront",
  "Changes": [
    {
      "Action": "CREATE",
      "ResourceRecordSet": {
        "Name": "your-custom-domain.com",
        "Type": "CNAME",
        "TTL": 300,
        "ResourceRecords": [
          {
            "Value": "YOUR-CLOUDFRONT-DOMAIN.cloudfront.net"
          }
        ]
      }
    }
  ]
}
